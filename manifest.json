{
  "manifest_version": 2,
  "name": "ascii emoticons",
  "short_name": "ascii emoticons",
  "description": "A Chrome app for searching ascii emoticons or emojis",
  "version": "0.0.2",
  "minimum_chrome_version": "42",

  "icons": [
    {
      "src": "assets/ascii-emoticons-logo-256.png",
      "sizes": "256x256",
      "type": "image/png"
    }
  ],

  "permissions": [
    "clipboardWrite"
  ],
  
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#FFEB3B"

}
